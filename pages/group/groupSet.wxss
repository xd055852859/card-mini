/* pages/group/groupSet.wxss */

page {
  background: #f3f2f7;
}

.setContent {
  margin-top: 20rpx;
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  font-size:34rpx;
  color:#333;
}

.setContent>view {
  width: 95%;
  height: 94rpx;
  margin-left: 5%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  
}
.setContent view:nth-child(1){
  border-bottom:1px solid #E5E5E5;
}
.masterImage {
  width: 60rpx;
  height: 60rpx;
  border: 1px solid #efefef;
  margin-right:20rpx;
}

.icon {
  width: 16rpx;
  height: 26rpx;
  margin-right:36rpx;
}

.masterView{
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.emptyView {
    width: 100%;
    height: 290rpx;
    position: absolute;
    top:335rpx;
    left:0rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    margin: 100rpx 0;
}

.emptyImage {
    width: 279rpx;
    height: 238rpx;
    margin-bottom: 15rpx;
}
.outButton ,.saveButton{
  width: 90%;
  height: 98rpx;
  position: fixed;
  left: 5%;
  bottom: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color:#727272;
  
  background: transparent;
  border-radius: 8rpx;
}
.saveButton{
    background: #5095FA;
    bottom: 158rpx;
    color: #fff;
}
.ics {
    width: 45rpx;
    height: 45rpx;
    margin-right: 10rpx;
  }