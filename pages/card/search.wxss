


.searchCon{display: flex;font-size: 24rpx;flex-direction: column; background:#F3F2F7;color:#fff;padding:30rpx;box-shadow: 2px 2px 6px 0}
.searchCon .splace{color:#aaa;text-align: left;font-size: 34rpx;}
.searchCon .scan{width:70rpx;height:70rpx;margin-right:30rpx;}
.searchCon .radio-group{margin:0 30rpx;}
.searchCon .s-c-ipt {width:100%;position:relative;margin-bottom:30rpx;}
.searchCon .s-c-ipt .sicons{width:40rpx;height:40rpx;position: absolute;left:20rpx;top:27rpx;z-index: 9;}
.searchCon .s-c-ipt input{background:#fff;color:#979797;border-radius: 16rpx;padding:0 76rpx;height:88rpx;line-height: 88rpx;min-height:88rpx;font-size: 34rpx;}
.searchCon  .s-choose{color:#333;display: flex;align-items: center;font-size: 32rpx;justify-content: flex-start}
.searchCon  .s-choose .radio{margin-right:70rpx;}
.searchCon  .s-choose .radio .icon{width:35rpx;height:35rpx;margin-right:10rpx;top:8rpx;position: relative}
.searchCon  .s-choose .radio radio{display: none}
.searchCon .iconsClear{position: absolute;top:24rpx;right:15rpx;z-index: 2}
.cardCon{padding:20rpx 30rpx;}
.cardCon .eachitems{padding-bottom:20rpx;margin-bottom:20rpx;position: relative}
.cardCon .eachitems:after{ content: ''; display:block; position: absolute; width: 100%; left: 0px; bottom: 0px;height: 1px; background-color: #E5E5E5; -webkit-transform: scaleY(0.5); transform: scaleY(0.5);}
.cardCon .eachitems  .navgt{display: flex;}
.cardCon .eachitems .header{width:90rpx;height:90rpx;margin-right:30rpx;}
.cardCon .eachitems .header image{width:100%;height:100%}
.cardCon .eachitems .details{flex: 1;font-size: 28rpx;flex-direction: column;justify-content: space-between;display:flex;overflow:hidden}
.cardCon .eachitems .dis{display: flex;align-items: center;color:#333;}
.cardCon .eachitems .com{font-size: 26rpx;color:#999;}
.cardCon .eachitems .dis .name{font-size: 36rpx;margin-right:10rpx;word-break:break-all;}
.cardCon .eachitems .dis .pos{color:#999;padding:0 10rpx;height:40rpx;line-height: 40rpx;overflow: hidden;text-overflow: ellipsis;white-space:nowrap;}
.cardCon .eachitems .iconsChange{position: absolute;right:0;top:46rpx;background:#222;width:100rpx;height:50rpx;background:#1CAC1B;color:#fff;border-radius: 16rpx;font-size: 28rpx;text-align: center;line-height: 50rpx;}
.noDataCon{color:#b2b2b2;font-size: 24rpx;text-align: center;}
.noDataCon .tipImg{width:292rpx;height:252rpx;margin:60rpx 30rpx 30rpx;}